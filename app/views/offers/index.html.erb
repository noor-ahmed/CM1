<p id="notice"><%= notice %></p>

<h1>Offers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Offer pulse</th>
      <th>Offer on net tariff</th>
      <th>Offer off net tariff,</th>
      <th>Offer atw,</th>
      <th>Offer call destination</th>
      <th>Offer priority</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offers.each do |offer| %>
      <tr>
        <td><%= offer.name %></td>
        <td><%= offer.offer_pulse %></td>
        <td><%= offer.offer_on_net_tariff %></td>
        <td><%= offer.offer_off_net_tariff %></td>
        <td><%= offer.offer_atw %></td>
        <td><%= offer.offer_call_destination %></td>
        <td><%= offer.offer_priority %></td>
        <td><%= link_to 'Show', offer %></td>
        <td><%= link_to 'Edit', edit_offer_path(offer) %></td>
        <td><%= link_to 'Destroy', offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offer', new_offer_path %>
